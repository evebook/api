    "lint": "lerna exec --no-bail -- tslint -p tsconfig.json",
    "test": "lerna run --no-bail test",
    "build": "lerna run --no-bail build",
    "build:docker": "yarn run build:docker:base && lerna run build:docker --concurrency=1",
